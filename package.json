{
  "name": "healthy-person-emulator-dotorg",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "prisma generate && remix vite:build",
    "dev": "remix vite:dev --host 0.0.0.0 --port 3000",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "remix-serve ./build/server/index.js",
    "seed": "npx tsx prisma/seed.ts",
    "reset:db": "npx prisma migrate reset -f && npx prisma db push",
    "typecheck": "tsc",
    "knip": "knip"
  },
  "dependencies": {
    "@clerk/localizations": "^2.4.5",
    "@clerk/remix": "^4.1.2",
    "@marsidev/react-turnstile": "^0.5.3",
    "@prisma/client": "^5.12.0",
    "@remix-run/node": "^2.8.1",
    "@remix-run/react": "^2.8.1",
    "@remix-run/serve": "^2.8.1",
    "@supabase/supabase-js": "^2.40.0",
    "chalk": "^5.3.0",
    "clsx": "^2.1.0",
    "crypto-js": "^4.2.0",
    "csv-parse": "^5.5.5",
    "diff": "^5.2.0",
    "html-react-parser": "^5.1.9",
    "is-ip": "^5.0.1",
    "isbot": "^4.1.0",
    "marked": "4.0",
    "node-html-markdown": "^1.3.0",
    "openai": "^4.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.2.1",
    "react-markdown": "^9.0.1",
    "remark-gfm": "^4.0.0",
    "remix-utils": "^7.5.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.9.0",
    "@playwright/test": "^1.42.1",
    "@remix-run/dev": "^2.8.1",
    "@types/diff": "^5.2.0",
    "@types/marked": "^6.0.0",
    "@types/node": "^20.14.13",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "autoprefixer": "^10.4.19",
    "daisyui": "^4.10.2",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "knip": "^5.27.0",
    "postcss": "^8.4.38",
    "prisma": "^5.11.0",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.7.2",
    "typescript": "^5.5.4",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
